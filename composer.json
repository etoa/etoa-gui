{
    "name": "etoa/etoa",
    "description": "Escape to Andromeda",
    "license": "proprietary",
    "repositories": [
        {"type": "vcs", "url": "https://github.com/etoa/Silex"}
    ],
    "require": {
        "php": "^8",
        "doctrine/dbal": "^3.0",
        "littlecubiclegames/quests": "^1.3",
        "monolog/monolog": "^3.0",
        "pimple/pimple": "^3.0",
        "silex/silex": "dev-t/symfony-53",
        "symfony/stopwatch": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/http-foundation": "^5.4",
        "twig/twig": "^3.3",
        "league/commonmark": "^2.0",
        "symfony/error-handler": "^5.4",
        "whichbrowser/parser": "^2.1",
        "symfony/lock": "^5.4",
        "symfony/flex": "^2.0",
        "symfony/monolog-bundle": "^3.7",
        "doctrine/doctrine-bundle": "^2.4",
        "symfony/proxy-manager-bridge": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "twig/extra-bundle": "^3.0",
        "symfony/yaml": "^5.4",
        "symfony/security-bundle": "^5.4",
        "doctrine/annotations": "^1.13",
        "symfony/translation": "^5.4",
        "scheb/2fa": "^6.0",
        "symfony/mime": "^5.4",
        "symfony/messenger": "^5.4",
        "symfony/amqp-messenger": "^5.4",
        "dragonmantank/cron-expression": "^3.1",
        "symfony/http-client": "^5.4",
        "twig/string-extra": "^3.3",
        "symfony/form": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/webpack-encore-bundle": "^1.13",
        "symfony/ux-live-component": "^2.0",
        "sensio/framework-extra-bundle": "^6.2",
        "endroid/qr-code": "^4.4",
        "symfony/mailer": "^6.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1",
        "phpstan/phpstan-phpunit": "^1",
        "phpstan/phpstan-strict-rules": "^1",
        "phpstan/phpstan-deprecation-rules": "^0.12.6|^1",
        "phpstan/phpstan-webmozart-assert": "^1",
        "phpunit/phpunit": "^9.0",
        "symfony/browser-kit": "^5.4",
        "rector/rector": "^0.13",
        "symfony/dotenv": "^5.4",
        "symfony/maker-bundle": "^1.33",
        "symfony/web-profiler-bundle": "^5.4",
        "symfony/phpunit-bridge": "^5.4",
        "phpstan/phpstan-doctrine": "^1.1",
        "phpstan/phpstan-symfony": "^1.1",
        "staabm/phpstan-dba": "^0.2.18"
    },
    "autoload": {
        "psr-4": {
            "EtoA\\": "src/"
        },
        "classmap": [
            "htdocs/classes"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EtoA\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "symfony/flex": true
        },
        "platform": {
            "php": "8.0.21"
        }
    }
}
