{
    "name": "etoa/etoa",
    "description": "Escape to Andromeda",
    "license": "proprietary",
    "repositories": [
        {"type": "vcs", "url": "https://github.com/etoa/Silex"}
    ],
    "require": {
        "php": "^8",
        "doctrine/dbal": "^2.13",
        "littlecubiclegames/quests": "^1.3",
        "monolog/monolog": "^2.3",
        "pimple/pimple": "^3.0",
        "silex/silex": "dev-t/symfony-53",
        "symfony/stopwatch": "^5.3",
        "symfony/http-kernel": "^5.3",
        "symfony/http-foundation": "^5.3",
        "twig/twig": "^3.3",
        "league/commonmark": "^1.3",
        "symfony/error-handler": "^5.3",
        "whichbrowser/parser": "^2.1",
        "swiftmailer/swiftmailer": "6.0",
        "symfony/lock": "^5.3",
        "symfony/flex": "^1.15",
        "symfony/monolog-bundle": "^3.7",
        "doctrine/doctrine-bundle": "^2.4",
        "symfony/proxy-manager-bridge": "^5.3",
        "symfony/twig-bundle": "^5.3",
        "twig/extra-bundle": "^3.0",
        "symfony/yaml": "^5.3",
        "symfony/security-bundle": "^5.3",
        "doctrine/annotations": "^1.13",
        "symfony/translation": "^5.3",
        "scheb/2fa": "^5.11",
        "scheb/2fa-qr-code": "^5.11",
        "symfony/mime": "^5.3",
        "symfony/messenger": "^5.3",
        "dragonmantank/cron-expression": "^3.1",
        "symfony/http-client": "^5.3",
        "twig/string-extra": "^3.3",
        "symfony/form": "^5.3",
        "symfony/validator": "^5.3",
        "symfony/webpack-encore-bundle": "^1.13",
        "symfony/ux-live-component": "^2.0",
        "sensio/framework-extra-bundle": "^6.2"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12.0",
        "phpstan/phpstan-strict-rules": "^0.12.0",
        "phpstan/phpstan-deprecation-rules": "^0.12.6",
        "phpunit/phpunit": "^9.0",
        "symfony/browser-kit": "^5.3",
        "rector/rector": "^0.11.16",
        "symfony/dotenv": "^5.3",
        "symfony/maker-bundle": "^1.33",
        "symfony/web-profiler-bundle": "^5.3",
        "symfony/phpunit-bridge": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "EtoA\\": "src/"
        },
        "classmap": [
            "htdocs/classes"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EtoA\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "symfony/flex": true
        }
    }
}
